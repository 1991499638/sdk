{
  "name": "@ston-fi/sdk",
  "version": "0.2.0",
  "description": "Typescript SDK to interact with the Ston.fi DEX",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ston-fi/sdk.git"
  },
  "homepage": "https://github.com/ston-fi/sdk#readme",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "build": "CI=true yarn lint && yarn test && vite build",
    "lint": "yarn lint:prettier",
    "lint:fix": "yarn lint:prettier --write",
    "lint:prettier": "prettier --check ./src",
    "test": "yarn test:typecheck && yarn test:unit-test",
    "test:typecheck": "tsc --noEmit",
    "test:unit-test": "vitest",
    "test:coverage": "yarn test:unit-test --coverage"
  },
  "dependencies": {
    "tonweb": "0.0.50"
  },
  "devDependencies": {
    "@types/bn.js": "5.1.1",
    "@types/node": "^18.11.7",
    "@vitest/coverage-c8": "^0.28.4",
    "prettier": "^2.8.1",
    "typescript": "4.8.4",
    "vite": "^4.0.0",
    "vite-plugin-dts": "1.7.1",
    "vitest": "^0.25.6"
  }
}
